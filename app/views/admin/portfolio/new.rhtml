<h1>New Porfolio Image</h1>

<%= error_messages_for :image %>

<% form_for(:image, :url => admin_portfolio_index_path,:html => { :multipart => true }) do |f| %>

  <p><b>title</b><i>(optional)</i><br/>
    <%= f.text_field(:title) %>
  </p>
  <p><b>upload photo</b><br/>
    <%= f.file_field("data") %>
  </p>  
  <p>
    <%= submit_tag "Upload" %>
  </p>
<% end %>


<%= link_to 'Cancel', admin_portfolio_index_path %>